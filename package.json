{
  "name": "office-time-tracker",
  "version": "2.0.11",
  "description": "An Angular and Node Express Typescript starter",
  "main": "index.js",
  "scripts": {
    "install:all": "concurrently \"npm install\" \"cd ./public/office-time-tracker && npm install\"",
    "ng:start": "cd ./public/office-time-tracker && tsc && npm start",
    "express:start": "tsc-watch --onSuccess 'nodemon --exec babel-node ./bin/www'",
    "start": "concurrently \"npm run express:start\"  \"npm run ng:start\"",
    "knex:latest": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:latest --knexfile knexfile.ts",
    "knex:rollback": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:rollback --knexfile knexfile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KShewengerz/office-time-tracker.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "express",
    "babel",
    "express typescript",
    "angular express"
  ],
  "author": "Kristy Mae Almuete <kristyalmuete@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KShewengerz/office-time-tracker/issues"
  },
  "homepage": "https://github.com/KShewengerz/office-time-tracker#readme",
  "dependencies": {
    "@angular/cli": "^7.3.8",
    "@types/jsonwebtoken": "^8.3.2",
    "babel-plugin-module-resolver": "^3.1.3",
    "bcrypt": "^3.0.5",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "camelcase-keys": "^5.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "serve-favicon": "~2.4.2",
    "snakecase-keys": "^2.1.0",
    "ts-node": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.42",
    "@types/express-session": "^1.15.8",
    "@types/jasmine": "^2.8.7",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.9",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.9.1",
    "typescript": "^3.2.4",
    "typescript-register": "^1.1.0"
  }
}
